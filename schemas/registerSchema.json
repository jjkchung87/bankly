{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email",
        "phone"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "new_user"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "new_password"
            ]
        },
        "first_name": {
            "type": "string",
            "default": "",
            "title": "The first_name Schema",
            "examples": [
                "new_first"
            ]
        },
        "last_name": {
            "type": "string",
            "default": "",
            "title": "The last_name Schema",
            "examples": [
                "new_last"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "new@newuser.com"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "1233211221"
            ]
        }
    },
    "examples": [{
        "username": "new_user",
        "password": "new_password",
        "first_name": "new_first",
        "last_name": "new_last",
        "email": "new@newuser.com",
        "phone": "1233211221"
    }]
}